  "components": {
    "schemas": {
      "ThumbnailsDto": {
        "type": "object",
        "properties": {
          "default": {
            "type": "string"
          },
          "medium": {
            "type": "string"
          },
          "high": {
            "type": "string"
          },
          "maxRes": {
            "type": "string"
          },
          "standard": {
            "type": "string"
          }
        },
        "required": [
          "default",
          "medium",
          "high",
          "maxRes",
          "standard"
        ]
      },
      "ChannelDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "aggregatedStats": {
            "type": "number"
          },
          "uploadsPlaylistId": {
            "type": "string"
          },
          "shouldBeInjested": {
            "type": "boolean"
          },
          "joystreamId": {
            "type": "number"
          },
          "thumbnails": {
            "$ref": "#/components/schemas/ThumbnailsDto"
          }
        },
        "required": [
          "title",
          "description",
          "aggregatedStats",
          "uploadsPlaylistId",
          "shouldBeInjested",
          "joystreamId",
          "thumbnails"
        ]
      },
      "UpdateChannelDto": {
        "type": "object",
        "properties": {
          "shouldBeIngested": {
            "type": "boolean"
          }
        },
        "required": [
          "shouldBeIngested"
        ]
      },
      "VideoDto": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "playlistId": {
            "type": "string"
          },
          "resourceId": {
            "type": "string"
          },
          "channelId": {
            "type": "string"
          },
          "thumbnails": {
            "$ref": "#/components/schemas/ThumbnailsDto"
          },
          "state": {
            "type": "object"
          },
          "destinationUrl": {
            "type": "string"
          }
        },
        "required": [
          "url",
          "title",
          "description",
          "id",
          "playlistId",
          "resourceId",
          "channelId",
          "thumbnails",
          "state",
          "destinationUrl"
        ]
      },
      "UserCreateRequest": {
        "type": "object",
        "properties": {
          "authorizationCode": {
            "type": "string"
          }
        },
        "required": [
          "authorizationCode"
        ]
      },
      "MembershipDto": {
        "type": "object",
        "properties": {
          "memberId": {
            "type": "object"
          },
          "address": {
            "type": "string"
          }
        },
        "required": [
          "memberId",
          "address"
        ]
      },
      "UserDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "avatarUrl": {
            "type": "string"
          },
          "channelsCount": {
            "type": "number"
          },
          "membership": {
            "$ref": "#/components/schemas/MembershipDto"
          }
        },
        "required": [
          "id",
          "email",
          "avatarUrl",
          "channelsCount",
          "membership"
        ]
      },
      "UserCreateResponse": {
        "type": "object",
        "properties": {
          "user": {
            "$ref": "#/components/schemas/UserDto"
          },
          "channels": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ChannelDto"
            }
          }
        },
        "required": [
          "user",
          "channels"
        ]
      },
      "CreateMembershipDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string"
          }
        },
        "required": [
          "userId"
        ]
      },
      "CreateJoystreamChannelDto": {
        "type": "object",
        "properties": {
          "channelId": {
            "type": "string"
          },
          "userId": {
            "type": "string"
          }
        },
        "required": [
          "channelId",
          "userId"
        ]
      },
      "CreateJoystreamVideoDto": {
        "type": "object",
        "properties": {
          "channelId": {
            "type": "string"
          },
          "userId": {
            "type": "string"
          },
          "videoId": {
            "type": "string"
          }
        },
        "required": [
          "channelId",
          "userId",
          "videoId"
        ]
      }
    }
