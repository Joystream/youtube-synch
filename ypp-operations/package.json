{
  "name": "ypp-operations",
  "version": "1.0.0",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "patch-package",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "build": "rm -rf lib && tsc --build tsconfig.json",
    "start": "node lib/index.js",
    "start:pm2": "pm2 start lib/index.js --name cronJob",
    "stop:pm2": "pm2 delete cronJob",
    "lint": "eslint ./src --ext .ts",
    "generate:docs:cli": "yarn oclif-dev readme --multi --dir ./docs/cli"
  },
  "files": [
    "/oclif.manifest.json"
  ],
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node-cron": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "pm2": "^5.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@hubspot/api-client": "^9.0.0",
    "@joystream/cli": "1.2.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/lokijs": "^1.5.14",
    "@types/uuid": "^9.0.1",
    "aws-sdk": "^2.1405.0",
    "axios": "^1.4.0",
    "chalk": "^4.0.0",
    "dotenv": "10.0.0",
    "dynamoose": "^3.1.0",
    "lokijs": "^1.5.12",
    "node-cron": "^3.0.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "uuid": "^9.0.0"
  },
  "oclif": {
    "repositoryPrefix": "<%- repo %>/blob/main/src/cli/<%- commandPath %>",
    "commands": "./lib/cli/commands",
    "bin": "youtube-synch",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {}
  },
  "bin": {
    "youtube-synch-service": "./bin/run"
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": "^1.22.15"
  },
  "volta": {
    "node": "14.18.0",
    "yarn": "1.22.15"
  },
  "resolutions": {
    "@polkadot/api": "8.9.1",
    "@polkadot/keyring": "9.5.1",
    "@polkadot/util": "9.5.1",
    "@polkadot/util-crypto": "9.5.1",
    "@substrate/txwrapper-core": "3.1.3",
    "@substrate/txwrapper-substrate": "3.1.3",
    "@substrate/txwrapper-polkadot": "3.1.3"
  }
}
