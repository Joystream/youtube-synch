  "info": {
    "title": "Joystream Youtube Sync API",
    "description": "Youtube Sync API",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "YtVideo": {
        "type": "object",
        "properties": {}
      },
      "SaveChannelRequest": {
        "type": "object",
        "properties": {
          "youtubeVideoUrl": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "joystreamChannelId": {
            "type": "number"
          },
          "shouldBeIngested": {
            "type": "boolean"
          },
          "videoCategoryId": {
            "type": "string"
          },
          "referrerChannelId": {
            "type": "number"
          }
        },
        "required": [
          "youtubeVideoUrl",
          "id",
          "email",
          "joystreamChannelId",
          "shouldBeIngested",
          "videoCategoryId"
        ]
      },
      "ThumbnailsDto": {
        "type": "object",
        "properties": {
          "default": {
            "type": "string"
          },
          "medium": {
            "type": "string"
          },
          "high": {
            "type": "string"
          }
        },
        "required": [
          "default",
          "medium",
          "high"
        ]
      },
      "ChannelDto": {
        "type": "object",
        "properties": {
          "youtubeChannelId": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "shouldBeIngested": {
            "type": "boolean"
          },
          "yppStatus": {
            "type": "string",
            "enum": [
              "Unverified",
              "Verified::Bronze",
              "Verified::Silver",
              "Verified::Gold",
              "Verified::Diamond",
              "Suspended::CopyrightBreach",
              "Suspended::MisleadingContent",
              "Suspended::ProgramTermsExploit",
              "Suspended::UnsupportedTopic",
              "OptedOut"
            ]
          },
          "joystreamChannelId": {
            "type": "number"
          },
          "referrerChannelId": {
            "type": "number"
          },
          "videoCategoryId": {
            "type": "string"
          },
          "language": {
            "type": "string"
          },
          "thumbnails": {
            "$ref": "#/components/schemas/ThumbnailsDto"
          },
          "subscribersCount": {
            "type": "number"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "syncStatus": {
            "type": "object"
          }
        },
        "required": [
          "youtubeChannelId",
          "title",
          "description",
          "shouldBeIngested",
          "yppStatus",
          "joystreamChannelId",
          "referrerChannelId",
          "videoCategoryId",
          "language",
          "thumbnails",
          "subscribersCount",
          "createdAt",
          "syncStatus"
        ]
      },
      "SaveChannelResponse": {
        "type": "object",
        "properties": {
          "channel": {
            "$ref": "#/components/schemas/ChannelDto"
          }
        },
        "required": [
          "channel"
        ]
      },
      "IngestChannelMessage": {
        "type": "object",
        "properties": {
          "videoCategoryId": {
            "type": "string"
          }
        },
        "required": [
          "videoCategoryId"
        ]
      },
      "IngestChannelDto": {
        "type": "object",
        "properties": {
          "signature": {
            "type": "string"
          },
          "message": {
            "$ref": "#/components/schemas/IngestChannelMessage"
          }
        },
        "required": [
          "signature",
          "message"
        ]
      },
      "OptoutChannelMessage": {
        "type": "object",
        "properties": {}
      },
      "OptoutChannelDto": {
        "type": "object",
        "properties": {
          "signature": {
            "type": "string"
          },
          "message": {
            "$ref": "#/components/schemas/OptoutChannelMessage"
          }
        },
        "required": [
          "signature",
          "message"
        ]
      },
      "UpdateChannelCategoryMessage": {
        "type": "object",
        "properties": {
          "videoCategoryId": {
            "type": "string"
          }
        },
        "required": [
          "videoCategoryId"
        ]
      },
      "UpdateChannelCategoryDto": {
        "type": "object",
        "properties": {
          "signature": {
            "type": "string"
          },
          "message": {
            "$ref": "#/components/schemas/UpdateChannelCategoryMessage"
          }
        },
        "required": [
          "signature",
          "message"
        ]
      },
      "SuspendChannelDto": {
        "type": "object",
        "properties": {
          "joystreamChannelId": {
            "type": "number"
          },
          "reason": {
            "type": "string",
            "enum": [
              "CopyrightBreach",
              "MisleadingContent",
              "ProgramTermsExploit",
              "UnsupportedTopic"
            ]
          }
        },
        "required": [
          "joystreamChannelId",
          "reason"
        ]
      },
      "VerifyChannelDto": {
        "type": "object",
        "properties": {
          "joystreamChannelId": {
            "type": "number"
          },
          "tier": {
            "type": "string",
            "enum": [
              "Bronze",
              "Silver",
              "Gold",
              "Diamond"
            ]
          }
        },
        "required": [
          "joystreamChannelId",
          "tier"
        ]
      },
      "SetOperatorIngestionStatusDto": {
        "type": "object",
        "properties": {
          "joystreamChannelId": {
            "type": "number"
          },
          "allowOperatorIngestion": {
            "type": "boolean"
          }
        },
        "required": [
          "joystreamChannelId",
          "allowOperatorIngestion"
        ]
      },
      "SetChannelCategoryByOperatorDto": {
        "type": "object",
        "properties": {
          "joystreamChannelId": {
            "type": "number"
          },
          "videoCategoryId": {
            "type": "string"
          }
        },
        "required": [
          "joystreamChannelId",
          "videoCategoryId"
        ]
      },
      "WhitelistChannelDto": {
        "type": "object",
        "properties": {
          "channelHandle": {
            "type": "string"
          }
        },
        "required": [
          "channelHandle"
        ]
      },
      "InductionRequirement": {
        "type": "object",
        "properties": {
          "template": {
            "type": "string",
            "description": "Template for the signup requirement text"
          },
          "variables": {
            "description": "Variables for requirement template",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "errorCode": {
            "type": "string",
            "description": "Error code to be returned when channel signup fails due to unmet requirement",
            "enum": [
              "CHANNEL_NOT_FOUND",
              "VIDEO_NOT_FOUND",
              "VIDEO_PRIVACY_STATUS_NOT_UNLISTED",
              "VIDEO_TITLE_MISMATCH",
              "CHANNEL_ALREADY_REGISTERED",
              "CHANNEL_STATUS_SUSPENDED",
              "CHANNEL_CRITERIA_UNMET_SUBSCRIBERS",
              "CHANNEL_CRITERIA_UNMET_VIDEOS",
              "CHANNEL_CRITERIA_UNMET_NEW_VIDEOS_REQUIREMENT",
              "CHANNEL_CRITERIA_UNMET_CREATION_DATE",
              "YOUTUBE_QUOTA_LIMIT_EXCEEDED",
              "YOUTUBE_API_NOT_CONNECTED"
            ]
          }
        },
        "required": [
          "template",
          "variables",
          "errorCode"
        ]
      },
      "ChannelInductionRequirementsDto": {
        "type": "object",
        "properties": {
          "requirements": {
            "description": "List of requirements user YT channel needs to fulfill",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/InductionRequirement"
            }
          }
        },
        "required": [
          "requirements"
        ]
      },
      "TopReferrerDto": {
        "type": "object",
        "properties": {
          "referrerChannelId": {
            "type": "number"
          },
          "referredByTier": {
            "type": "object"
          },
          "totalEarnings": {
            "type": "number"
          },
          "totalReferredChannels": {
            "type": "number"
          }
        },
        "required": [
          "referrerChannelId",
          "referredByTier",
          "totalEarnings",
          "totalReferredChannels"
        ]
      },
      "VerifyChannelRequest": {
        "type": "object",
        "properties": {
          "youtubeVideoUrl": {
            "type": "string"
          }
        },
        "required": [
          "youtubeVideoUrl"
        ]
      },
      "VerifyChannelResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "channelHandle": {
            "type": "string"
          },
          "channelTitle": {
            "type": "string"
          },
          "channelDescription": {
            "type": "string"
          },
          "channelLanguage": {
            "type": "string"
          },
          "avatarUrl": {
            "type": "string"
          },
          "bannerUrl": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "channelHandle",
          "channelTitle",
          "channelDescription",
          "channelLanguage",
          "avatarUrl",
          "bannerUrl"
        ]
      },
