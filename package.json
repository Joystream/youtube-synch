{
  "name": "youtube-sync",
  "version": "3.1.0",
  "license": "MIT",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "build": "rm -rf lib && tsc --build tsconfig.json && cp -R ./src/infrastructure/pulumi ./lib/infrastructure/pulumi",
    "start": "DEPLOYMENT_ENV=dev ./bin/run start",
    "lint": "eslint ./src --ext .ts",
    "start:prod": "./bin/run start",
    "docker:start": "DEPLOYMENT_ENV=dev IMAGE_TAG=$(./scripts/code-shasum.sh) docker-compose up -d",
    "docker:start:prod": "IMAGE_TAG=$(./scripts/code-shasum.sh) docker-compose up -d",
    "dynamodb:local:start": "yarn dynamodb:local:stop && export DEPLOYMENT_ENV=local && ./src/infrastructure/deploy.sh",
    "dynamodb:local:stop": "export DEPLOYMENT_ENV=local REMOVE_PULUMI_STACK=true && ./src/infrastructure/destroy.sh",
    "generate:docs:cli": "yarn oclif-dev readme --multi --dir ./docs/cli",
    "generate:docs:config": "yarn ts-node ./src/schemas/scripts/generateConfigDoc.ts",
    "generate:docs:all": "yarn generate:docs:cli && yarn generate:docs:config",
    "generate:types:all": "yarn generate:types:json-schema && yarn generate:types:graphql",
    "generate:types:graphql": "npx graphql-codegen --config ./src/services/query-node/codegen.yml",
    "generate:types:json-schema": "yarn ts-node ./src/schemas/scripts/generateTypes.ts",
    "generate:all": "yarn generate:docs:all && yarn generate:types:all"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    ".env",
    "config.yml",
    "docker-compose.yml",
    "yarn.lock"
  ],
  "main": "lib/index.js",
  "oclif": {
    "repositoryPrefix": "<%- repo %>/blob/main/src/cli/<%- commandPath %>",
    "commands": "./lib/cli/commands",
    "bin": "youtube-synch",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {}
  },
  "bin": {
    "youtube-synch-service": "./bin/run"
  },
  "devDependencies": {
    "@adobe/jsonschema2md": "https://github.com/adobe/jsonschema2md#3926af64f38409811a8f4627e95557199c9c8c71",
